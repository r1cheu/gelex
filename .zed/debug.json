 // Project-local debug tasks
     //
     // For more documentation on how to configure debug tasks,
     // see: https://zed.dev/docs/debugger
    [
        {
            "label": "Debug gelex assoc",
            "program": "$ZED_WORKTREE_ROOT/build/debug/src/gelex",
            "args": [
                "assoc",
                "--pheno",
                "remove_gs3.txt",
                "--bfile",
                "data/311_all_rmdup",
                "--grm",
                "./data/real.add",
                "--chunk-size",
                "10000",
                "--pheno-col",
                "13",
                "--threads",
                "8",
                "--out",
                "yield.d",
                "--model",
                "d",
                "--qcovar",
                "./data/plink2.eigenvec",
            ],

            "cwd": "$ZED_WORKTREE_ROOT/develop",
            "request": "launch",
            "adapter": "CodeLLDB",
            "build": {
                "command": "pixi",
                "args": [
                    "r",
                    "build-debug"
                ],
                "cwd": "$ZED_WORKTREE_ROOT",
            },
        },
    ]
